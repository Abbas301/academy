{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0", 
    "title": "MediFit App",
    "description": "Medifit For Healthy Life"
  },
  "host": "localhost:2000",
  "basePath": "/api",
  "tags": [
    {
      "name": "Default",
      "description": "API for Medifit Application"
    }
  ],
  "schemes": ["http","https"],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "definitions":{
    "login":{
      "type":"object",
      "properties":{
        "email":{
          "type":"string",
          "description":"Email ID To Login",
          "example":"Example@gmail.com"
        },
        "password":{
          "type":"string",
          "description":"Enter A Valid Password",
          "example":"sanjkjhb"
        }
      }
    }
  },
  "paths": {
    "/register": {
      "get": {
        "tags": [
            "Default"
        ],
        "parameters":[
          {
            "name":"x-auth-token",
            "in":"header",
            "description":"Authentication to login",
            "required":true,
            "type":"string"
          }
        ],
        "summary": "Get all users in system",
        "responses": {
          "200": {
            "description": "Ok"
          },
          "400":{
            "description": "Bad request"
          },
          "404":{
            "description": "Page Not Found or File Not Found"
          },
          "401":{
              "description": "Unauthorized Client"
          }
        }
      }
    }
  }
  
}
